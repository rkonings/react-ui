{
    "name": "@randykonings/react-ui",
    "version": "0.0.6",
    "description": "React UI Elements",
    "scripts": {
        "test": "jest",
        "test:coverage": "jest --coverage",
        "storybook": "start-storybook --port 6006",
        "build-storybook": "build-storybook -c .storybook -o .out",
        "build": "rm -rf /build && tsc",
        "build:icons": "svgr --out-dir src/Icon/ --template svgr.template.js --ext tsx src/assets/icons/",
        "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
        "lint:style": "stylelint 'src/**/*.{ts,tsx,js}'",
        "lint:fix": "tslint --fix -p ./",
        "predeploy": "npm run build-storybook",
        "deploy-storybook": "gh-pages -d .out"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rkonings/react-ui.git"
    },
    "keywords": [
        "React",
        "UI",
        "elements"
    ],
    "author": "Randy Konings",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/rkonings/react-ui/issues"
